      $ "Refuge Policy Tracking.IsUKLSpilling" [] := IF ( $ "Upper Klamath Lake.Spill" [] > 0.00000000 "cfs" )
 THEN
  1.00000000
 ELSE
  0.00000000
 ENDIF;

      $ "Refuge Policy Tracking.IsKenoSpilling" [] := IF ( $ "Keno.Spill" [] > 0.00000000 "cfs" )
 THEN
  1.00000000
 ELSE
  0.00000000
 ENDIF;

    END
    UUID "{9649acb1-e84e-41bd-8f12-fe433c8207ae}";;

  END
